  "scripts": {
    "manualBuild": "yarn localize && d2-manifest package.json ./public/manifest.webapp --manifest.activities.dhis.href='../../..' && GENERATE_SOURCEMAP=false react-app-rewired build && mkdirp buildzip/ && pushd build && bestzip ../buildzip/NewCaptureApp.zip * && popd",
    "start": "react-app-rewired start",
    "build": "yarn localize && d2-manifest package.json ./public/manifest.webapp && GENERATE_SOURCEMAP=false react-app-rewired build && cp ./package.json ./build/package.json",
    "test": "react-app-rewired test --env=jsdom",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject",
    "add-flow-types": "flow-typed install",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/",
    "prestart": "yarn localize && d2-manifest package.json ./public/manifest.webapp",
    "docs": "NODE_ENV=development jsdoc -c jsdoc-conf.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn extract-pot && git add -A ."
    }
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bestzip": "^2.1.2",
    "docdash": "^1.0.3",
