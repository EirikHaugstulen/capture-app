{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "dhis2",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "globals": {
        "document": true,
        "window": true,
        "$": true
    },
    "rules": {
        "no-param-reassign": 0       
    },
    "settings": {
         "import/resolver": {
            "webpack": {
                "config": "./config/eslintWebpack.config.js"
            }
        }
    }
}